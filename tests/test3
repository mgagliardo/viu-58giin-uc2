int x;
int y;
int z;
x = 2;
while(x > 0)
{
y= 2;
	while(y > 0)
	{
	z = 2;
		while(z > 0)
		{
			if(x == y && y == z){
				System.out.println(x);
				System.out.println(y);
				System.out.println(z);
			}else
			{
				System.out.println(0);
			}
			z = z - 1;	
		
		}
		y =  y -1;
	
	}
	x = x - 1;

}
System.out.println(55);